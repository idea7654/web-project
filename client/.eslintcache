ss\";\nimport { withRouter } from \"react-router-dom\";\nimport axios from \"axios\";\nconst CarouselComponent = ({ history }) => {\n  const handleClick = (id) => {\n    history.push(`/detail/${id}`);\n  };\n\n  const [Product, setProduct] = useState(null);\n\n  useEffect(() => {\n    axios.get(\"http://localhost:8000/api/posts\").then((res) => {\n      setProduct(res.data);\n    });\n  }, []);\n\n  const Product1 = (\n    <div onClick={() => handleClick(Product[Product.length - 1].id)}>\n      {Product ? <img src={Product[Product.length - 1].img[0].image} /> : \"\"}\n    </div>\n  );\n\n  const Product2 = (\n    <div onClick={() => handleClick(Product[Product.length - 2].id)}>\n      {Product ? <img src={Product[Product.length - 2].img[0].image} /> : \"\"}\n    </div>\n  );\n\n  const Product3 = (\n    <div onClick={() => handleClick(Product[Product.length - 3].id)}>\n      {Product ? <img src={Product[Product.length - 3].img[0].image} /> : \"\"}\n    </div>\n  );\n  return (\n    <div className=\"carousel-wrapper\">\n      <Carousel>\n        {Product1}\n        {Product2}\n        {Product3}\n      </Carousel>\n    </div>\n  );\n};\n\nexport default withRouter(CarouselComponent);\n","/Users/oh/Desktop/Project/web-project/client/src/index.js",[],["150","151"],"/Users/oh/Desktop/Project/web-project/client/src/context/context.js",[],"/Users/oh/Desktop/Project/web-project/client/src/components/Section/Register.js",["152"],"import React, { useState, useContext } from \"react\";\nimport axios from \"axios\";\nimport { UserContext } from \"../../context/context\";\nconst Register = ({ history }) => {\n  const [Id, setId] = useState(\"\");\n  const [Password, setPassword] = useState(\"\");\n  const [User, setUser] = useContext(UserContext);\n  const changeId = (e) => {\n    setId(e.target.value);\n  };\n\n  const changePassword = (e) => {\n    setPassword(e.target.value);\n  };\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    let body = {\n      username: Id,\n      password: Password,\n    };\n    axios\n      .post(\"http://localhost:8000/api/auth/register/\", body)\n      .then((res) => {\n        //console.log(res);\n        setUser(res.data);\n        history.push(\"/\");\n      })\n      .catch((err) => {\n        alert(\"아이디 혹은 비밀번호의 조건이 맞지 않습니다.\");\n      });\n  };\n  return (\n    <div className=\"h-screen flex justify-center\">\n      <div className=\"flex items-center justify-center\">\n        <div className=\"w-full max-w-md\">\n          <form className=\"bg-white rounded px-12 pt-6 pb-8 mb-4\">\n            {/* <!-- @csrf --> */}\n            <div className=\"text-gray-800 text-2xl flex justify-center border-b-2 py-2 mb-4\">\n              Register\n            </div>\n            <div className=\"mb-4\">\n              <label\n                className=\"block text-gray-700 text-sm font-normal mb-2\"\n                htmlFor=\"username\"\n              >\n                아이디(4자 이상)\n              </label>\n              <input\n                className=\"shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline\"\n                type=\"text\"\n                required\n                autoFocus\n                placeholder=\"Id\"\n                value={Id}\n                onChange={changeId}\n              />\n            </div>\n            <div className=\"mb-6\">\n              <label\n                className=\"block text-gray-700 text-sm font-normal mb-2\"\n                htmlFor=\"password\"\n              >\n                Password(6자 이상)\n              </label>\n              <input\n                className=\"shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 mb-3 leading-tight focus:outline-none focus:shadow-outline\"\n                v-model=\"form.password\"\n                type=\"password\"\n                placeholder=\"Password\"\n                name=\"password\"\n                required\n   