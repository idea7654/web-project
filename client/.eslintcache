ntent\", Context);\n    await axios\n      .post(\"http://localhost:8000/api/posts/\", formData, {\n        headers: {\n          Authorization: User.token,\n        },\n      })\n      .then((res) => {\n        console.log(res);\n      });\n  };\n\n  return (\n    <div className=\"m-12 flex flex-col justify-between\">\n      <div>\n        <div>\n          <div className=\"mb-3\">제목</div>\n          <input\n            value={Title}\n            onChange={titleChange}\n            className=\"border-solid border-4 border-light-blue-500 mb-6 w-full py-1\"\n            type=\"text\"\n            placeholder=\"제목을 입력해주세요\"\n          />\n        </div>\n        <div>\n          <div className=\"mb-3\">제품 이름</div>\n          <input\n            value={Pname}\n            onChange={pnameChange}\n            className=\"border-solid border-4 border-light-blue-500 mb-6 w-full py-1\"\n            type=\"text\"\n            placeholder=\"제품 이름을 입력해주세요\"\n          />\n        </div>\n        <div>\n          <div className=\"mb-3\">카테고리</div>\n          <Dropdown\n            Category={Category}\n            setCategory={setCategory}\n            color=\"white\"\n          />\n        </div>\n        <div>\n          <div className=\"mb-3\">제품 정보</div>\n          <textarea\n            value={Context}\n            onChange={contextChange}\n            className=\"border-solid border-4 border-light-blue-500 mb-6 w-full py-1\"\n            type=\"text\"\n            placeholder=\"제품 정보를 입력해주세요\"\n          />\n        </div>\n        <div>\n          <div className=\"mb-3\">이미지</div>\n          <input\n            onChange={imageChange}\n            className=\"border-solid border-4 border-light-blue-500 w-full mb-6 py-1\"\n            type=\"file\"\n            accept=\"images/*\"\n            multiple\n          />\n          {Content\n            ? Preview.map((data) => {\n                return (\n                  <div>\n                    <img src={data} alt=\"\" />\n                  </div>\n                );\n              })\n            : \"\"}\n        </div>\n      </div>\n      <div className=\"flex justify-between\">\n        <button\n          onClick={() => setUpdateFlag(false)}\n          className=\"bg-blue-700 text-white border border-blue-700 font-bold py-2 px-6 rounded-lg\"\n        >\n          취소\n        </button>\n        <button\n          onClick={handleSubmit}\n          className=\"bg-blue-700 text-white border border-blue-700 font-bold py-2 px-6 rounded-lg\"\n        >\n          등록\n        </button>\n      </div>\n    </div>\n  );\n};\n\nexport default Update;\n",["99","100"],"/Users/oh/Desktop/Project/web-project/client/src/components/Section/Register.js",["101"],"/Users/oh/Desktop/Project/web-project/client/src/components/Navbar/Navbar.js",["102","103","104","105"],"/Users/oh/Desktop/Project/web-project/client/src/components/Section/Ar.js",["106"],"/Users/oh/Desktop/Project/web-project/client/src/components/Section/Reply.js",["107","108"],{"ruleId":"109","severity":1,"message":"110","line":8,"column":10,"nodeType":"111","messageId":"112","endLine":8,"endColumn":14},{"ruleId":"113","severity":1,"message":"114","line":86,"column":13,"nodeType":"115","endLine":89,"endColumn":14},{"ruleId":"116","severity":1,"message":"117","line":13,"column":13,"nodeType":"118","endLine":22,"endColumn":4},{"ruleId":"116","severity":1,"message":"119","line":22,"column":6,"nodeType":"120","endLine":22,"endColumn":8,"suggestions":"121"},{"ruleId":"113","severity":1,"message":"114","line":34,"column":21,"nodeType":"115","endLine":34,"endColumn":61},{"ruleId":"113","severity":1,"message":"122","line":44,"column":25,"nodeType":"115","endLine":47,"endColumn":26},{"ruleId":"109","severity":1,"message":"123","line":13,"column":16,"nodeType":"111","messageId":"112","endLine":13,"endColumn":23},{"ruleId":"109","severity":1,"message":"124","line":3,"column":8,"nodeType":"111","messageId":"112","endLine":3,"endColumn":13},{"ruleId":"